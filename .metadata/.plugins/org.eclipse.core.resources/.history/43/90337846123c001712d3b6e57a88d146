#include <iostream>
using namespace std;

class Point{
private:
	int x, y;

public:
	int count;

	Point()
	{
		count++;
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
