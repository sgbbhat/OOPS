#include <iostream>
using namespace std;

class Base
{
public:
	void show()
	{
		cout<<"In base class"<<endl;
	}
};

class Derived
{
public:
	void show()
	{
		cout<<"In Derived class"<<endl;
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
