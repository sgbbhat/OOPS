#include <iostream>
#include <cstdlib>
using namespace std;

// Typedef
typedef char test;

// Using
using integer = int;

// Unions
union mytypes_t {
  char c;
  int i;
  float f;
};

// enumerated type
enum enum_t
{
	black,
	blue,
	green,
	cyan
};

int main() {
	test x = '1';
	integer i = 10;

	mytypes_t mybyte;

	mybyte.f = 20;

	cout<<mybyte.f<<endl;
	//cout<<mybyte.c<<endl;
	cout<<mybyte.i<<endl;

	enum_t mycolor;

	mycolor = 2;

	return 0;
}
