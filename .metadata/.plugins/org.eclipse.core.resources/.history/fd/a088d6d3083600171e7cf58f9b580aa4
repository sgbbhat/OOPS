#include <iostream>
using namespace std;

class Shape
{
protected:
	int height, width;

public:
	void set_height(int h)
	{
		height = h;
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
