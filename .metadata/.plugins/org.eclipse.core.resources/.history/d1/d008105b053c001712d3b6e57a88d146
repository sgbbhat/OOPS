#include <iostream>
using namespace std;

class FunOv
{
public:
	void print(int val)
	{
		cout<<"Value passed is"<< val<<" is of type int"<<endl;
	}

	void print(float val)
	{
		cout<<"Value passed is"<< val<<" is of type float"<<endl;
	}

	void print(char val)


};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
