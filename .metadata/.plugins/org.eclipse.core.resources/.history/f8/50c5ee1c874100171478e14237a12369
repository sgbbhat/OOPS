#include <iostream>
using namespace std;

void print(int * a)
{
	for(int i=0; i <10; i++)
	{
		cout<<a[i] <<endl;
	}
}

int main() {
	int * a;
	a = (int *)malloc(10*sizeof(int));

	for(int i=0; i <10; i++)
	{
		a[i] = 100 + i;
	}

	print(a);

	return 0;
}
