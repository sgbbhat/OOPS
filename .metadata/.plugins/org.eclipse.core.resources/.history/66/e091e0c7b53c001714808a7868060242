#include <iostream>
using namespace std;

class Base
{
	int id;
	float balence;

public:
	void printinfo()
	{
		cout<<"Printing base"<<endl;
	}
};

class Savings:public Base
{
	int age;
	long int ATM;

public:
	void printinfo()
	{
		cout<<"Printing Savings"<<endl;
	}

};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
