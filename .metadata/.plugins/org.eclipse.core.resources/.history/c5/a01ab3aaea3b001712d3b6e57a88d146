#include <iostream>
using namespace std;

class Line{
private:
	int * ptr;

public:
	int getLength(void);
	Line(int len);
	Line(const Line &obj)
	~Line
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
