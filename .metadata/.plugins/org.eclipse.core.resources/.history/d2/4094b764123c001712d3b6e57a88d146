#include <iostream>
using namespace std;

class Point{
private:
	int x, y;

public:
	int count;

	Point()
	{
		count++;
	}

	Point(int a, int b)
	{
		x = a;
		y = b;
		count++;
	}

	void printCount(){
		cout<<"Count : "<<count<<endl;
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
