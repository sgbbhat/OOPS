#include <iostream>
using namespace std;

class Base
{
public:
	int id;
	float balance;

	Base(int identity, float bal){
		id = identity ;
		balance = bal;
	}

	void printinfo()
	{
		cout<<"Printing in base"<<endl;
		cout<<"Id = "<<id<<endl;
		cout<<"Bal = "<<balance<<endl;
	}
};

class Savings
{
public:
	void printinfo()
	{
		cout<<"Printing in base"<<endl;
		cout<<"Id = "<<id<<endl;
		cout<<"Bal = "<<balance<<endl;
	}

};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
