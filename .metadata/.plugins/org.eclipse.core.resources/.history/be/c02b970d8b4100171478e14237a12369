#include <iostream>
using namespace std;

class Line
{
private:
	int * ptr;

public:
	Line(const Line &obj){
		ptr = new int;
		*ptr = obj.ptr;
	}

	int getLength( void ) {
	   return *ptr;
	}
};

int main() {
	return 0;
}
