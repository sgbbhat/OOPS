#include <iostream>
using namespace std;

class channel
{
private:
	char * c;

public:
	channel(char ch){
		c = ch;
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
