#include <iostream>
using namespace std;

class Point{
private:
	int x, y;

public:
	friend bool colinear(Point &p1, Point &p2, Point &p3)
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
