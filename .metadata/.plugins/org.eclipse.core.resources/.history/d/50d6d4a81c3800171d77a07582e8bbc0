#include <iostream>
using namespace std;

// Swap by pointer
void swapByPtr(int *a, int *b)
{
	int temp;
	temp = *a;
	*a = *b;
	*b = *a;
}

// Swap by reference
void swapByRef(int &a, int &b)
{
	int temp;
	temp = a;
	a = b;
	b = a;
}

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
