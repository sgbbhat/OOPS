#include <iostream>
using namespace std;

class Line
{
private:
	int * ptr;

public:
	Line(const Line &obj){

	}

	int getLength( void ) {
	   return *ptr;
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
